<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestNG Regression Suite" verbose="3">
	
  <test name="Google Test">
	  <parameter name="url" value="https://www.google.com/"></parameter> 
	  <parameter name="browser" value="chrome"></parameter> 
    <classes>
      <class name="uiTests.GoogleTest"/>
    </classes>
  </test> 
  
  <test name="OrangeHRM Test">
	  <parameter name="url" value="https://www.orangehrm.com/30-day-free-trial/"></parameter> 
	  <parameter name="browser" value="firefox"></parameter> 
    <classes>
      <class name="uiTests.OrangeHRMTest"/>
    </classes>
  </test> 
  
  <test name="OpenCart Test">
	  <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/login"></parameter> 
	  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="uiTests.LoginTestBeforeTest"/>
    </classes>
  </test> 
  
  
</suite> 